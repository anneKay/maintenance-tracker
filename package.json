{
  "name": "maintenance-tracker",
  "version": "1.0.0",
  "description": "Maintenance Tracker App is an application that provides users with the ability to reach out to operations or repairs department regarding repair or maintenance requests and monitor the status of their request.",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=lcov --reporter=text-lcov mocha --compilers js:babel-core/register **/*testfile.js",
    "test-watch": "nodemon --exec 'npm test'",
    "babel": "babel --presets es2015 server/routes/dbroutes.js -o build/routes/dbroutes.js",
    "start": "babel-node --presets=es2015 server/app.js",
    "dev-start": "nodemon --exec babel-node --presets=es2015 server/app.js",
    "coverage": "babel-node ./node_modules/istanbul/lib/cli cover _mocha server/tests/test.js",
    "lint": "eslint server/app.js",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test | coveralls",
    "stop-process": "taskkill /F /IM node.exe",
    "setup-tables": "babel-node --presets=es2015 server/database/setupTables.js",
    "seed-admin": "babel-node --presets=es2015 server/database/seedAdmin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anneKay/maintenance-tracker.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anneKay/maintenance-tracker/issues"
  },
  "homepage": "https://github.com/anneKay/maintenance-tracker#readme",
  "devDependencies": {
    "coveralls": "^2.13.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "expect": "^1.20.2",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.17.4",
    "nyc": "^11.8.0",
    "supertest": "^2.0.1"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16",
    "joi": "^13.3.0",
    "jsonwebtoken": "^8.2.1",
    "pg": "^7.4.3"
  }
}
