{
  "name": "maintenance-tracker",
  "version": "1.0.0",
  "description": "Maintenance Tracker App is an application that provides users with the ability to reach out to operations or repairs department regarding repair or maintenance requests and monitor the status of their request.",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=lcov mocha --compilers js:babel-core/register **/*testfile.js",
    "test-front": "jest --coverage -u",
    "test-watch": "nodemon --exec 'npm test'",
    "babel": "babel --presets es2015 server/routes/dbroutes.js -o build/routes/dbroutes.js",
    "babel-react": "babel --presets=react react/src/class_react.js --out-file=react/scripts/app.js --presets=env,react --watch",
    "build": "webpack --production",
    "start": "babel-node --presets=es2015 server/app.js",
    "heroku-postbuild": "npm run build",
    "dev-server": "webpack-dev-server",
    "dev-start": "nodemon --exec babel-node --presets=es2015 server/app.js",
    "coverage": "babel-node ./node_modules/istanbul/lib/cli cover _mocha server/tests/testfile.js",
    "lint": "eslint server/app.js",
    "live": "live-server public",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test | coveralls",
    "stop-process": "taskkill /F /IM node.exe",
    "setup-tables": "babel-node --presets=es2015 server/database/setupTables.js",
    "seed-admin": "babel-node --presets=es2015 server/database/seedAdmin.js"
  },
  "engines": {
    "node": "8.9.4"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/front_react/src/tests/testSetup.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/front_react/src/tests/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anneKay/maintenance-tracker.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anneKay/maintenance-tracker/issues"
  },
  "homepage": "https://github.com/anneKay/maintenance-tracker#readme",
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "coveralls": "^2.13.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "expect": "^1.20.2",
    "identity-obj-proxy": "^3.0.0",
    "jwt-decode": "^2.2.0",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.17.4",
    "nyc": "^11.8.0",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.3.5",
    "supertest": "^2.0.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "clean-webpack-plugin": "^0.1.19",
    "cors": "^2.8.4",
    "css-loader": "^1.0.0",
    "dotenv": "^5.0.1",
    "express": "^4.16",
    "file-loader": "^2.0.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "joi": "^13.3.0",
    "jsonwebtoken": "^8.2.1",
    "live-server": "^1.2.0",
    "mini-css-extract-plugin": "^0.4.3",
    "moment": "^2.22.2",
    "moxios": "^0.4.0",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pg": "^7.4.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-modal": "^3.5.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "semantic-ui-css": "^2.3.3",
    "semantic-ui-react": "^0.82.5",
    "style-loader": "^0.23.0",
    "swagger-ui": "^3.17.0",
    "swagger-ui-dist": "^3.17.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
    "validator": "^10.7.1",
    "webpack-dev-server": "^3.1.6"
  }
}
